{

    //configuração de tipo de execução
    "AppMode": 0, // 0=Single Run Mode 1=Dynamic mode 2=Batch test mode

    //configurações de exibição e ferramentas para SingleRunMode e BatchMode
    "MapViwer": 0, // 0=Console 1=OpenGL
    "MapOrigin": 1, // 0=Static 1=FromFile 2=Random 3=Standard
    "Heuristic": 0, // 0=Manhatam 1=Euclidean 2=Octile 3=Chebyshev
    "AllowDiagonal": 1, // 0=Never 1=OnlyWhenNoObstacles 2=IfAtMostOneObstacle 3=Always
    "Algorithm": 4, // 0=AStar 1=BFS 2=Dijkstra 3=IDA* 4=GA

    //IDA Params
    "IDAStarFinderTimeOut": 5000, // in ms
    "IDATrackRecursion": true,

    // Random Map Generator
    "RandomSeed": 0.4,
    "Width": 40,
    "Height": 40,
    "MinimumPath": 15,
    "RandomBlock": 5,

    // Symbols Map Console e input map
    "FileToLoad": "10.txt",
    "FolderToSaveMaps": "Maps/",

    "Start": "S",
    "End": "E",
    "Empty": "-",
    "Wall": "#",
    "Path": "*",
    "Closed": "C",
    "Opened": "O",

    // OpenGL Map Params
    "OpenGlBlockSize": 15,

    // COnfigurações de batch
    "Batch_map_origin": 0, // 0=generate new 1=load from folder
    "Batch_map_qtd_to_generate": 100, 
    "Batch_generate_pattern": 1, //0=random 1=pattern
    "Batch_GATimesToRunPerMap": 5,
    "Batch_folder": "Batch/"

}
