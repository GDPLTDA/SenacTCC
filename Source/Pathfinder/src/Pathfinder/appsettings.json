{

  //configuração de tipo de execução
  "AppMode": 2, // 0=Single Run Mode 1=Dynamic mode 2=Batch test mode
  
  //configurações de exibição e ferramentas para SingleRunMode e BatchMode
  "MapViwer": 1, // 0=Console 1=OpenGL
  "MapOrigin": 2, // 0=Static 1=FromFile 2=Random
  "Heuristic": 0, // 0=Manhatam 1=Euclidean 2=Octile 3=Chebyshev
  "AllowDiagonal": 0, // 0=Never 1=OnlyWhenNoObstacles 2=IfAtMostOneObstacle 3=Always
  "Algorithn": 1, // 0=AStar 1=BFS 2=Dijkstra 3=IDA* 4=GA

  //IDA Params
  "IDAStarFinderTimeOut": 10000, // in ms
  "IDATrackRecursion": true,


  // Random Map Generator
  "RandomSeed": 0.3,
  "Width": 20,
  "Height": 20,
  "MinimumPath": 8,

  // Symbols Map Console e input map
  "FileToLoad": "Maps/test.txt",
  "FolderToSaveMaps": "Maps/",

  "Start": "S",
  "End": "E",
  "Empty": "-",
  "Wall": "#",
  "Path": "*",
  "Closed": "C",
  "Opened": "O",


  // OpenGL Map Params
  "OpenGlBlockSize": 10,

  // COnfigurações de batch
  "Batch_map_origin": 0, // 0=generate new 1=load from folder
  "Batch_map_qtd_to_generate": 10, // 0=generate new 1=load from folder
  "Batch_folder": "Batch/"

}