{
  //configurações de exibição e ferramentas
  "MapViwer": 1, // 0=Console 1=OpenGL
  "MapOrigin": 2, // 0=Static 1=FromFile 2=Random
  "Heuristic": 0, // 0=Manhatam 1=Euclidean 2=Octile 3=Chebyshev
  "AllowDiagonal": 0, // 0=Never 1=OnlyWhenNoObstacles 2=IfAtMostOneObstacle 3=Always
  "Algorithn": 3, // 0=AStar 1=BFS 2=Dijkstra 3=IDA* 4=GA

  //IDA Params
  "IDAStarFinderTimeOut": 1000, // in ms
  "IDATrackRecursion": true, // in ms

  // Symbols Map Console e input map
  "Start": "S",
  "End": "E",
  "Empty": "-",
  "Wall": "#",
  "Path": "*",
  "Closed": "C",
  "Opened": "O",

  // Random Map Generator
  "RandomSeed": 0.4,
  "Width": 20,
  "Height": 20,
  "MinimumPath": 10,

  // OpenGL Map Params
  "OpenGlBlockSize": 30
}